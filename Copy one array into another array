#include<stdio.h>
int main()
{
   int a[100],b[100],i,n;
   printf("\n enter the limit:");
   scanf("%d",&n);
   printf("\n enter the nelements in array:");
    for(i=0;i<n;i++)
   scanf("%d",&a[i]);
   
   for(i=0;i<n;i++)
   {
      b[i]=a[i];
   }
   printf("\n Copied array:");
     for(i=0;i<n;i++)
   printf("%d\t",b[i]);
}
